  "tokenColors":[
    {
      "name":"Comment",
      "scope":[
        "comment",
        "punctuation.definition.comment"
      ],
      "settings":{
        "fontStyle":"italic",
        "foreground":"#af9a6a"
      }
    },
    {
      "name":"Variables",
      "scope":[
        "variable",
        "string constant.other.placeholder"
      ],
      "settings":{
        "foreground":"#0db0ff"
      }
    },
    {
      "name":"Colors",
      "scope":[
        "constant.other.color"
      ],
      "settings":{
        "foreground":"#3faa26"
      }
    },
    {
      "name":"Invalid",
      "scope":[
        "invalid",
        "invalid.illegal"
      ],
      "settings":{
        "foreground":"#cf7a7a"
      }
    },
    {
      "name":"Keyword, Storage",
      "scope":[
        "keyword",
        "storage.type",
        "storage.modifier"
      ],
      "settings":{
        "foreground":"#cfaf00"
      }
    },
    {
      "name":"Operator, Misc",
